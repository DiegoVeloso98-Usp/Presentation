
@article{Ferreira2024,
  author =        {Ferreira, A R and Marengo, A and Perego, U},
  journal =       {(under revision)},
  title =         {A Phase-Field Gradient-Based Energy Split for
                   Modeling of Brittle Fracture under Cyclic Loading},
  year =          {2024},
  abstract =      {Despite the attention devoted to phase-field modeling
                   of brittle fracture in the last 20 years, there are
                   still some open issues. Among these, the search for a
                   physically reasonable and computationally feasible
                   criterion to split the energy density into parcels
                   that may or may not contribute to crack propagation
                   has been the subject of many recent studies. Within
                   this context, we propose an energy split -- or energy
                   decomposition -- with the aim of representing as
                   accurately as possible the mechanical behavior of a
                   crack under cyclic loading. To this purpose, two key
                   assumptions are made. The first one is the
                   interpretation of the gradient damage direction as
                   being equivalent to the normal-to-crack direction, as
                   already known by the literature. The second
                   assumption is to consider the sign of the projection
                   of the strain tensor on the normal-to-crack
                   directions at a point as an indicator of whether this
                   point should behave as an open or as a closed crack.
                   We associate the latter case to both (a) a complete
                   recovery of elastic energy density and (b) a zero
                   driving force at such point. The first case is
                   treated classically as a damageable material point at
                   which the crack is free to propagate. Four examples
                   are presented in order to assess the suitability of
                   the present model by using both AT1 and AT2
                   regularization models. Results show the desired
                   effect of limiting crack propagation to tension as
                   well as of recovering its intact stiffness upon load
                   reversal, even when the two most known energy splits
                   fail to. Finally, the simplicity and effectiveness of
                   our approach may pave the way to more sophisticated
                   idealizations and to complex large-scale
                   applications.},
  timestamp =     {2024-07-18T14:02:50Z},
}

@article{Marengo2021,
	title = {A rigorous and efficient explicit algorithm for irreversibility enforcement in phase-field finite element modeling of brittle crack propagation},
	issn = {00457825},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0045782521004680},
	abstract = {In the present work, a computationally efficient and explicit algorithm for the rigorous enforcement of the irreversibility constraint in the phase-field modeling of brittle fracture is presented. The proposed approach is staggered and relies on the alternate minimization of the total energy functional. The phase-field evolution turns out to be governed by a complementarity boundary-value problem, where the complementarity stems from the irreversibility, while the boundary-value problem is originated by the presence of the gradient term in the phase-field functional. Several different techniques have been proposed in the literature to account for damage irreversibility in a computationally effective way. Following a similar approach proposed in the past for a gradient-plasticity problem, a particularly simple and effective solution strategy based on the Projected Successive Over-Relaxation (PSOR) method for constrained optimization, where an iterative explicit scheme is used for the solution of symmetric linear complementarity problems, is presented. Even though the proposed method is restricted to linear complementarity problems, it can be applied to the numerical simulation of a wide range of problems discussed in the literature. The performance of the suggested solution algorithm on a number of test cases is compared with that of a recently proposed penalty approach for the approximated enforcement of irreversibility.},
	language = {en},
	urldate = {2024-10-02},
	journal = {Computer Methods in Applied Mechanics and Engineering},
	author = {Marengo, Alessandro and Patton, Alessia and Negri, Matteo and Perego, Umberto and Reali, Alessandro},
	month = dec,
	year = {2021},
	pages = {114137},
}


