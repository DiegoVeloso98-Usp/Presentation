@article{kuhn_degradation_2015,
	title = {On degradation functions in phase field fracture models},
	volume = {108},
	copyright = {https://www.elsevier.com/tdm/userlicense/1.0/},
	issn = {0927-0256},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0927025615003602},
	doi = {10.1016/j.commatsci.2015.05.034},
	abstract = {Unlike the description of cracks as sharp surfaces which requires different criteria to predict the onset of crack propagation, the direction of crack growth, possible crack branching, and the nucleation of new cracks, the phase ﬁeld approach provides a holistic description of all these fracture processes. The phase ﬁeld order parameter is used to differentiate between broken and undamaged material, and the entire crack evolution is obtained implicitly by solving the evolution equation of the order parameter which is coupled to the mechanical ﬁeld equations. A so called degradation function couples the order parameter to the elastic properties of the material in order to model the change in stiffness between fractured and undamaged material. The nucleation of a new crack in originally undamaged material is preceded by a localization of the fracture ﬁeld. Before the onset of this localization the material response of the phase ﬁeld fracture model is mainly controlled by the degradation function. However, the degradation function frequently found in the literature yields a pronounced softening behavior before the onset of fracture which is not desirable when modeling brittle materials. In this work we discuss the potential of alternative degradation functions in the context of crack nucleation and propagation.},
	language = {en},
	urldate = {2025-07-11},
	journal = {Computational Materials Science},
	author = {Kuhn, Charlotte and Schlüter, Alexander and Müller, Ralf},
	month = oct,
	year = {2015},
	note = {Publisher: Elsevier BV},
	pages = {374--384},
	file = {PDF:/home/diego/Zotero/storage/LHZLJ9UD/Kuhn et al. - 2015 - On degradation functions in phase field fracture models.pdf:application/pdf},
}

